{
    "manifest_version": 2,
    "name": "__MSG_extName__",
    "description": "__MSG_extDesc__",
    "default_locale": "en",
    "version": "1.0.0",
    "icons": {
        "16": "icons/tab-dark-16.png",
        "32": "icons/tab-dark-32.png",
        "48": "icons/tab-dark-48.png",
        "128": "icons/tab-dark-128.png"
    },
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "permissions": [
        "storage",
        "tabs"
    ],
    "background": {
        "scripts": ["js/bg.js"],
        "persistent": false
    },
    "browser_action": {
        "default_title": "__MSG_extName__"
    },
    "homepage_url": "https://github.com/Vechro/Simple-New-Tab/"
}
